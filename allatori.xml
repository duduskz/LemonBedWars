<config>
    <input>
        <jar in="target/LemonBedWars-1.0.jar" out="target/LemonBedWars.jar"/>
    </input>

    <ignore-classes>
        <class template="class com.zaxxer.hikari.*"/>
    </ignore-classes>

    <keep-names>
        <class access="protected+">
            <field access="protected+"/>
            <method access="protected+"/>
        </class>
    </keep-names>


    <classpath>
        <jar name="/HikariCP.jar"/>
    </classpath>

    <property name="force-default-package" value="enable"/>
    <property name="classes-naming" value="abc"/>
    <property name="packages-naming" value="abc"/>
    <property name = "fields-naming-prefix" value ="abc"/>
    <property name ="update-resource-names" value ="enable"/>
</config>